<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content" >
		<LinearLayout android:layout_width="match_parent"
	    	android:layout_height="match_parent"
	    	android:orientation="horizontal">
	    	   	    
		<ImageView
		    android:id="@+id/tab_info"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="30dp"
		    android:contentDescription="@string/infoconsulta"
		    android:src="@drawable/ic_tab_info"
		    android:onClick="onClick"
		     />

		<ImageView
		    android:id="@+id/tab_fila"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="40dp"
		    android:onClick="onClick"
		    android:src="@drawable/ic_tab_fila"
		    android:contentDescription="@string/fila"
		    />
		 
		 <ImageView
		     android:id="@+id/tab_rota"
		     android:layout_width="wrap_content"
		     android:layout_height="wrap_content"
		     android:layout_marginLeft="40dp"
		     android:onClick="onClick"
		     android:src="@drawable/ic_tab_rota"
		     android:contentDescription="@string/rota"
		      />
		 
		 <ImageView
		     android:id="@+id/tab_chamada"
		     android:layout_width="wrap_content"
		     android:layout_height="wrap_content"
		     android:layout_marginLeft="40dp"
		     android:contentDescription="@string/chamada"
		     android:src="@drawable/ic_tab_chamada"
		     android:onClick="onClick"
		     />

	</LinearLayout>
		<View
		    android:id="@+id/divisor_alto"
	    	android:layout_width="match_parent"
	    	android:layout_height="1dp"
	    	android:background="@color/light_grey1" /> 
		
		 <View
		    android:id="@+id/divisor_baixo"
	    	android:layout_width="match_parent"
	    	android:layout_height="1dp"
	    	android:layout_marginTop="34dp"
	    	android:background="@color/light_grey1" />
</RelativeLayout>